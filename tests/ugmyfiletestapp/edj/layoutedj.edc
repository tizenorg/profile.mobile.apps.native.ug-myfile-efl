collections
{

   group
   {
      name: "mylayout";

      data
      {
         item: "title" "Layout ug test";
      }

      parts
      {
         part
         {
            name: "title";
            type: TEXT;

            description
            {
               state: "default" 0.0;
               color: 0 0 0 255;
               rel1
               {
                  relative: 0.0 0.0;
               }
               rel2
               {
                  relative: 0.2 1.0;
               }
               text
               {
                  text: "bla";
                  size: 16;
                  font: "sans";
                  min: 1 1;
               }
            }
         }

         part
         {
            name: "custom";
            type: SWALLOW;

            description
            {
               state: "default" 0.0;
               fixed: 1 1;

               rel1
               {
                  relative: 0.0 0.2;
               }
               rel2
               {
                  relative: 1.0 1.0;
               }
            }
         } // example/custom


      }
   }
}
